<h3>Admin Page</h1>
<table class="table">
  <thead class="thead-inverse">
    <tr>
      <th>#</th>
      <th>Name</th>
      <th>Last Name/th>
      <th>Email</th>
      <th>Role</th>
      <th>Confirmed</th>
      <th>Last Sign In</th>
      <th>Sign In Count</th>

    </tr>
  </thead>
  <% @users.each_with_index do |user, index|%>
  <tbody>
    <tr>
      <th scope="row"><%= (index + 1).to_s %></th>
      <td><%= user.name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
      <td><%= user.confirmed_at %></td>
      <td><%= user.last_sign_in_at %></td>
      <td><%= user.sign_in_count %></td>
      <td><%= button_to "DELETE", {controller: "registrations", action: "destroy", user_id: user.id}, method: :delete %></td>
    </tr>
  </tbody>
  <% end %>
</table>